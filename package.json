{
  "name": "annas-archive-scraper",
  "version": "1.0.0",
  "description": "A Node.js CLI tool for searching and downloading books using puppeteer-real-browser with Cloudflare bypass support.",
  "main": "scraper.js",
  "bin": {
    "annas-scraper": "./scraper.js"
  },
  "files": [
    "scraper.js",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "node scraper.js",
    "search": "node scraper.js",
    "test": "echo \"No tests specified\" && exit 0",
    "lint": "eslint *.js --fix",
    "lint:check": "eslint *.js",
    "dev": "nodemon scraper.js",
    "install:global": "npm install -g .",
    "uninstall:global": "npm uninstall -g annas-archive-scraper",
    "example": "node scraper.js \"The Great Gatsby\"",
    "help": "node scraper.js --help",
    "clean": "rm -rf node_modules package-lock.json && npm install",
    "prepare": "echo 'Preparing package for publication...'",
    "prepack": "npm run lint:check"
  },
  "keywords": [
    "scraper",
    "puppeteer",
    "cloudflare-bypass",
    "nodejs",
    "cli-tool",
    "automation"
  ],
  "author": {
    "name": "Vaibhav",
    "email": "your.email@example.com",
    "url": "https://github.com/vaibhav1405"
  },
  "contributors": [
    {
      "name": "Vaibhav",
      "email": "your.email@example.com",
      "url": "https://github.com/vaibhav1405"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vaibhav1405/annas-archive-scraper.git"
  },
  "bugs": {
    "url": "https://github.com/vaibhav1405/annas-archive-scraper/issues"
  },
  "homepage": "https://github.com/vaibhav1405/annas-archive-scraper#readme",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/vaibhav1405"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "dependencies": {
    "puppeteer-real-browser": "^1.3.1",
    "puppeteer-extra-plugin-click-and-wait": "^4.0.0"
  },
  "optionalDependencies": {},
  "peerDependencies": {},
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5"
  },
  "preferGlobal": true,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "config": {
    "unsafe-perm": true
  },
  "volta": {
    "node": "18.19.0",
    "npm": "10.2.3"
  }
}
